    <tabs>
      <tab idMso="TabMail">
        <group id="group1" label="myPlugins">
          <gallery id="SetCategory" imageMso="ColorMenu"
                   supertip="Assign message(s) to a category(ies)" label="Categorize" size="large"
                   getItemCount="catGallery_getItemCount"
